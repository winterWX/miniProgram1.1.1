page {
  width: 100%;
  height: 100%;
  background-color: #fff;
 }
  
 .circle-box {
  text-align: center;
  margin-top: 10vw;
 }
  
 .circle {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
 }
  
 .draw_btn {
  width: 28vw;
  position: absolute;
  top: 31vw;
  right: 0;
  left: 0;
  margin: auto;
  border: 1px #0D0D0D solid;
  background-color: #BFEFFF;
  border-radius: 5vw;
 }