<view class="mainContent">
  <view class="swiper-tab">
      <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}"  data-current="0"  bindtap="swichNav">未使用</view>
      <view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}"  data-current="1"  bindtap="swichNav">已过期</view>
      <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}"  data-current="2"  bindtap="swichNav">已使用</view>
  </view>
  <swiper current="{{currentTab}}" class="swiper-box" duration="300" style="height:{{winHeight - 31}}px" bindchange="bindChange">
      <swiper-item class="swpierItemContent">
          <view class="item-content" wx:for="{{detailArray}}" wx:key="index">
                <image  data-id="{{item.id}}"   class="item-contentImage" src="../../image/myCounty/quan@3x.png" />
                <view class="itemSecondeImage">
                    <!-- <image  class="itemMiddenImage" src="../../image/myCounty/cuhk-medical-centre-logo-hori-4-c@3x.png" />  -->
                    <image  class="itemMiddenImage" src="{{item.attachmentUrl}}" />
                    <view class="itemBlock">
                        <view class="itemBlockText1">{{item.name}}</view>
                        <view class="itemBlockText2">有效期至{{item.expiryTime}}</view>
                    </view>
                    <view class="itemBlockTextLast" wx:if="{{item.useTime === 0}}">未使用</view>
                </view>
          </view>
      </swiper-item>
      <swiper-item class="swpierItemContent">
        <view class="item-content" wx:for="{{detailArray}}" wx:key="index">
                <image  data-id="{{item.id}}"   class="item-contentImage" src="../../image/myCounty/invalid-name@3x.png" />
                <view class="itemSecondeImage">
                    <!-- <image  class="itemMiddenImage" src="../../image/myCounty/cuhk-medical-centre-logo-hori-4-c@3x.png" />  -->
                    <image  class="itemMiddenImage" src="{{item.attachmentUrl}}" />
                    <view class="itemBlock">
                        <view class="itemBlockText1">{{item.name}}</view>
                        <view class="itemBlockText2">有效期至{{item.expiryTime}}</view>
                    </view>
                    <image class="itemBlockTextLast" src="../../image/myCounty/invalid-name@3x.png" />
                    <!-- <view class="itemBlockTextLast" wx:if="{{item.useTime === 0}}">未使用</view> -->
                </view>
          </view>
      </swiper-item>
      <swiper-item class="swpierItemContent">
        <view class="item-content" wx:for="{{detailArray}}" wx:key="index">
                <image  data-id="{{item.id}}"   class="item-contentImage" src="../../image/myCounty/invalid-name@3x.png" />
                <view class="itemSecondeImage">
                    <!-- <image  class="itemMiddenImage" src="../../image/myCounty/cuhk-medical-centre-logo-hori-4-c@3x.png" />  -->
                    <image  class="itemMiddenImage" src="{{item.attachmentUrl}}" />
                    <view class="itemBlock">
                        <view class="itemBlockText1">{{item.name}}</view>
                        <view class="itemBlockText2">有效期至{{item.expiryTime}}</view>
                    </view>
                    <image class="itemBlockTextLast" src="../../image/myCounty/invalid-name@2x.png" />
                    <!-- <view class="itemBlockTextLast" wx:if="{{item.useTime === 0}}">未使用</view> -->
                </view>
          </view>
      </swiper-item>
  </swiper>
</view>