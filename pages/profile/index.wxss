
.healthBack{
   width: 50%;
   height: 200rpx;
   background-color: #00a865;
   color: black;
   font-size: 40rpx;
   display: flex;
   align-items: center;
   justify-content: center;
   border: 1rpx solid #ccc;
}

.content-box {
}

.progress-box {
  height: 106rpx;
  line-height: 106rpx;
  border-bottom: 2rpx solid #b8b6b6;
  padding: 0 38rpx 0 32rpx;
  position: relative;
}

.press-number-box {
  width: 60rpx;
  height: 60rpx;
  line-height: 52rpx;
  border-radius: 50%;
  background-color: #09BB07;
  color: white;
  text-align: center;
  position: absolute;
  top: 24rpx;
  left: 32rpx;
  z-index: 2;
}

.title {
  background-color: rgb(237, 237, 237);
  padding: 0 34rpx 0 30rpx;
  height: 72rpx;
  line-height: 72rpx;
}
.title-basic {
  font-size: 28rpx;
  line-height: 40rpx;
}
.title-tip {
  font-size: 24rpx;
  line-height: 40rpx;
  margin-left: 10rpx;
  color: rgb(131, 123, 123);
}

.press-number {
  font-size: 24rpx;
}

.progress-bar {
  position: absolute;
  top: 44rpx;
  width: 670rpx;
  left: 34rpx;
}

.gift {
  width: 58rpx;
  height: 55rpx;
  z-index: 3;
  position: absolute;
  bottom: 32rpx;
  right: 38rpx;
}

.userinfo-item-container {
  height: 106rpx;
  line-height: 106rpx;
  border-bottom: 2rpx solid #b8b6b6;
  padding: 0 30rpx 0 30rpx;
  background-color: white;
  display: flex;
  justify-content: space-between;
}
.avatar {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.userinfo-item-label {
  color: rgb(51, 51, 51);
  font-size: 28rpx;
}
.userinfo-item-value__label {
  color: rgb(51, 51, 51);
  font-size: 28rpx;
}

.userinfo-item-value__icon {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  transform: rotate(-180deg);
}
.memeber-label {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
}
.memeber-label-value {
  font-size: 26rpx;
  color: rgb(51, 51, 51);
}
.hase-member-font {
  font-size: 28rpx;
  color: rgb(51, 51, 51);
}
.mb10 {
  margin-bottom: 20rpx;
}
.disabled-color {
  color: rgb(146, 146, 146);
}