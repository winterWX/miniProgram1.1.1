page {
  height: 100%;
}
.container {
  background-color: #f3f3f3;
  padding-bottom: 110rpx;
}
.container .header {
  width: 100%;
  height: 720rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.container .header .tip-box {
  position: relative;
  height: 90rpx;
}
.container .header .tip-box .profile-tip {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  letter-spacing: -0.3rpx;
}
.container .header .tip-box .bg {
  width: 750rpx;
  height: 90rpx;
}
.container .header .tip-box .link {
  border-bottom: 2rpx solid #333;
}
.container .portrait {
  padding: 50rpx 0 30rpx 0;
  text-align: center;
  width: 100%;
}
.container .portrait .header-box {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  position: relative;
  margin-bottom: 20rpx;
}
.container .portrait .header-box .circle {
  width: 200rpx;
  height: 200rpx;
}
.container .portrait .header-box .avatar {
  width: 176rpx;
  height: 176rpx;
  position: absolute;
  top: 12rpx;
  left: 12rpx;
}
.container .portrait .header-box .edit-icon {
  width: 68rpx;
  height: 68rpx;
  position: absolute;
  bottom: 0rpx;
  right: 0rpx;
}
.container .portrait .name {
  text-align: center;
  font-size: 32rpx;
  color: #333;
}
.container .header .info {
  display: flex;
  align-items: center;
}
.container .header .info .item {
  flex: 1;
