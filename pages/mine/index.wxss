page {
  height: 100%;
}
.container {
  background-color: #f3f3f3;
  padding-bottom: 110rpx;
}
.container .header {
  width: 100%;
  /* height: 720rpx; */
  margin-bottom: 20rpx;
  background-color: #fff;
}
.container .header .tip-box {
  position: relative;
  height: 90rpx;
}
.container .header .tip-box .profile-tip {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
  letter-spacing: -0.3rpx;
}
.container .header .tip-box .bg {
  width: 750rpx;
  height: 90rpx;
}
.container .header .tip-box .link {
  border-bottom: 2rpx solid #333;
}
.container .portrait {
  padding: 50rpx 0 30rpx 0;
  text-align: center;
  width: 100%;
}
.container .portrait .header-box {
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
  position: relative;
  margin-bottom: 20rpx;
}
.container .portrait .header-box .circle {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}
.container .portrait .header-box .avatar {
  width: 176rpx;
  height: 176rpx;
  position: absolute;
  top: 13rpx;
  left: 13rpx;
}
.container .portrait .header-box .edit-icon {
  width: 68rpx;
  height: 68rpx;
  position: absolute;
  bottom: 0rpx;
  right: 0rpx;
}
.container .portrait .name {
  text-align: center;
  font-size: 32rpx;
  color: #333;
}
.container .header .info {
  display: flex;
  align-items: center;
}
.container .header .info .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.container .header .info .dividing-line {
  width: 2rpx;
  height: 60rpx;
  background-color: #d7d8d6;
}
.container .header .info .item .num {
  text-align: center;
  font-size: 48rpx;
  font-weight: 500;
  height: 70rpx;
  line-height: 70rpx;
  margin-bottom: 10rpx;
}
.container .header .info .item .tip {
  text-align: center;
  color: #666;
  font-size: 30rpx;
  height: 42rpx;
  line-height: 42rpx;
  position: relative;
}
.container .header .info .item .tip .icon {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 80rpx;
  top: -2rpx;
}
.container .member-info {
  text-align: center;
  margin-top: 40rpx;
  position: relative;
  height: 128rpx;
}
.container .member-info .img {
  width: 694rpx;
  height: 128rpx;
}
.container .member-info .level {
  position: absolute;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: 500;
  left: 120rpx;
  top: 50rpx;
}
.container .member-info .gold {
  color: #ffd478;
}
.goldBlock{
  color: #ffd478 !important;
}
.container .member-info .silver {
  color: #304352;
}
.container .member-info .copper {
  color: #0d0d0d;
}
.container .member-info .lever-icon {
  position: absolute;
  bottom: 36rpx;
  left: 62rpx;
  width: 46rpx;
  height: 46rpx;
}

.container .member-info .guide {
  position: absolute;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: 500;
  right: 72rpx;
  top: 50rpx;
}
.container .content {
  background-color: #f3f3f3;
  margin-bottom: 130rpx;
}
.container .guider-box {
  padding: 0 30rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.container .guider-box .item {
  border-bottom: 2rpx solid #f3f3f3;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
}
.container .guider-box .item .left {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .guider-box .item .right {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .guider-box .item .right .label {
  font-size: 36rpx;
  color: #666;
}
.container .guider-box .item .left .icon {
  width: 64rpx;
  height: 64rpx;
  margin-right: 26rpx;
}
.container .guider-box .item .left .label {
  font-size: 30rpx;
  color: #333;
}
.container .guider-box .item .right .tip {
  width: 16rpx;
  height: 16rpx;
  background-color: #b10d19;
  border-radius: 50%;
}
.container .guider-box .item .right .icon {
  width: 36rpx;
  height: 36rpx;
}
/*模态框*/
.modals {
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.modals-cancel {
  position: absolute;
  z-index: 1000;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
}
.bottom-dialog-body {
  position: absolute;
  z-index: 10001;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 30rpx 30rpx 0rpx 30rpx;
  min-height: 320rpx;
  background-color: #fff;
  border-radius: 30rpx 30rpx 0rpx 0rpx;
}
/*动画前初始位置*/
.bottom-pos {
-webkit-transform: translateY(100%);
transform: translateY(100%);
}
