  flex-direction: column;
  justify-content: space-between;
}
.container .header .info .dividing-line {
  width: 2rpx;
  height: 60rpx;
  background-color: #d7d8d6;
}
.container .header .info .item .num {
  text-align: center;
  font-size: 48rpx;
  font-weight: 500;
  height: 70rpx;
  line-height: 70rpx;
  margin-bottom: 10rpx;
}
.container .header .info .item .tip {
  text-align: center;
  color: #666;
  font-size: 30rpx;
  height: 42rpx;
  line-height: 42rpx;
